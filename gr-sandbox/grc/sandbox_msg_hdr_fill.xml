<block>
  <name>msg header + fill</name>
  <key>sandbox_msg_hdr_fill</key>
  <category>Sandbox</category>
  <import>import sandbox</import>
  <make>sandbox.msg_hdr_fill($fill_max)</make>

  	<param>
    		<name>fill max</name>
    		<key>fill_max</key>
    		<type>int</type>
  	</param>

	<sink>
		<name>in</name>
		<type>message</type>
        	<optional>1</optional>
	</sink>

	<source>
		<name>out</name>
		<type>message</type>
		<optional>1</optional>
	</source>

</block>
